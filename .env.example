# PayFast Configuration (Sandbox)
# These are the default PayFast test credentials - replace with your own for production

# PayFast Sandbox Credentials (Public Test Credentials)
VITE_PAYFAST_MERCHANT_ID=10000100
VITE_PAYFAST_MERCHANT_KEY=46f0cd694581a
VITE_PAYFAST_PASSPHRASE=jt7NOE43FZPn

# API Configuration
# For local development with Railway server
VITE_API_URL=http://localhost:8080
# For production, use your Railway domain
# VITE_API_URL=https://your-railway-domain.railway.app

# Environment
NODE_ENV=development

# App Configuration
VITE_APP_NAME=Young Eagles PWA
VITE_APP_URL=http://localhost:3000

# PayFast URLs (Sandbox)
VITE_PAYFAST_SANDBOX_URL=https://sandbox.payfast.co.za/eng/process
VITE_PAYFAST_PRODUCTION_URL=https://www.payfast.co.za/eng/process

# VAPID Keys for Push Notifications
# Get these from your Vercel dashboard or generate new ones
# Public key is safe to expose in frontend, private key should be in backend only
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key_here
# Note: VAPID_PRIVATE_KEY should be in your backend .env, not here

# Google AdSense Configuration
# Get your AdSense Publisher ID from your AdSense account
# Format: pub-xxxxxxxxxxxxxxxx
VITE_ADSENSE_PUBLISHER_ID=your_adsense_publisher_id_here

# AdSense Ad Unit IDs
# Create ad units in your AdSense account and replace these with your actual ad unit IDs
VITE_ADSENSE_BANNER_AD_UNIT=your_banner_ad_unit_id_here
VITE_ADSENSE_SIDEBAR_AD_UNIT=your_sidebar_ad_unit_id_here
VITE_ADSENSE_FOOTER_AD_UNIT=your_footer_ad_unit_id_here

# AdSense Settings
# Set to 'true' to enable ads in production, 'false' to disable
VITE_ADSENSE_ENABLED=false
# Set to 'true' for testing ads in development
VITE_ADSENSE_TEST_MODE=true
