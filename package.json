{
  "name": "young-eagles-pwa-minimal",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Young Eagles Progressive Web App - Minimal Version",
  "scripts": {
    "dev": "vite --port 3002",
    "build": "npm run clean && vite build",
    "build:production": "NODE_ENV=production npm run clean && vite build",
    "preview": "vite preview --port 3003",
    "serve": "npm run build && vite preview",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "test:build": "npm run build && npm run preview"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "axios": "^1.10.0",
    "crypto-js": "^4.2.0",
    "framer-motion": "^12.23.3",
    "hammerjs": "^2.0.8",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.8.1",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.30.1",
    "@squoosh/lib": "^0.5.3",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "vite": "^5.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
